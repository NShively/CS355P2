<% include header %>

    <div>
    <% if(rs.length > 0) { %>
        <table border="1">
            <tr><th>Title</th></tr>
            <% for (var i=0; rs.length > i; i++) { %>
                <tr>
                 <td><a href="/movies/?studentid=<%= rs[i].MovieID %>"> <%= rs[i].Title %></a></td>
                </tr>
            <% } %>
        </table>
    <% } else { %>
         No movies are entered into the Movies database table.
    <% } %>
    </div>

    <div style="margin-top:10px;">
        <a href="/movies/create">Create New Movie</a>
    </div>

<% include footer %>